<userDefinedSQL><![CDATA[CREATE OR REPLACE FORCE EDITIONABLE VIEW "V_SAW_HASIL" ("ID_SOFTWARE", "NAMA_SOFTWARE", "PEREFERENSI", "NILAI_SAW") AS 
  SELECT 
    sn.ID_software,
    sn.nama AS nama_software,
    sn.ID_pereferensi AS pereferensi,  
    ROUND(
        (sn.n1 * p.w1) +
        (sn.n2 * p.w2) +
        (sn.n3 * p.w3) +
        (sn.n4 * p.w4) +
        (sn.n5 * p.w5) , 10
    ) AS nilai_saw
FROM v_saw_normalisasi sn 
INNER JOIN pereferensi p ON p.ID.pereferensi = sn.ID.pereferensi
ORDER BY sn.ID_software,Â sn.ID_pereferensi;]]></userDefinedSQL>
